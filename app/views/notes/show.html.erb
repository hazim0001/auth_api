<div class="page-header">
  <%= link_to notes_path, class: 'btn btn-default' do %>
    All Notes
  <% end %>
  <%= link_to edit_note_path(@note), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show note</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @note.name %></dd>
  <dt>Description:</dt>
  <dd><%= @note.description %></dd>
  <dt>Quantity:</dt>
  <dd><%= @note.quantity %></dd>
  <dt>Due date:</dt>
  <dd><%= @note.due_date %></dd>
</dl>
